{
    "go": {
        "type": "exe",
        "version": "1.20.11",
        "installers": ["Tar"],
        "extract_path": "~/.local",
        "url":"https://go.dev/dl/go{version}.linux-amd64.tar.gz"
    },
    "gopls": {
        "type": "exe",
        "version": "0.18.1",
        "installers": ["bh", "Go"],
        "source_repo": "https://github.com/golang/tools/tree/master/gopls",
        "url": "golang.org/x/tools/gopls"
    },
    "golangci-lint": {
        "type": "exe",
        "version": "2.0.2",
        "installers": ["bh", "Go"],
        "homepage": "https://golangci-lint.run",
        "source_repo": "https://github.com/golangci/golangci-lint",
        "url": "github.com/golangci/golangci-lint/cmd/golangci-lint"
    },
    "ts-go": {
        "type": "parser",
        "language": "go"
    }
}
