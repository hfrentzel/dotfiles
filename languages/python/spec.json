{
    "python": {
        "type": "exe",
        "version": "3.11.11",
        "installers": [
            {
                "installer": "Apt",
                "package_name": [
                    "python3.11",
                    "python3.11-venv"
                ],
                "apt_repo": "ppa:deadsnakes/ppa",
                "post_script": [
                    "sudo update-alternatives --install /usr/bin/python python /usr/bin/python3.11 11",
                    "curl -sS https://bootstrap.pypa.io/get-pip.py | python3.11"
                ]
            }
        ],
        "homepage": "https://www.python.org/",
        "source_repo": "https://github.com/python/cpython"
    },
    "python_data": {
        "type": "directory",
        "path": "~/.local/share/python"
    },
    "startup.py": {
        "type": "symlink",
        "source": "DOT/languages/python/startup.py",
        "target": "~/.config/python/startup.py"
    },
    "python-lsp-server": {
        "type": "exe",
        "command_name": "pylsp",
        "version": "1.12.2",
        "installers": [
            "Pip"
        ],
        "source_repo": "https://github.com/python-lsp/python-lsp-server"
    },
    "ruff": {
        "type": "exe",
        "version": "0.9.7",
        "installers": [
            "Pip"
        ],
        "homepage": "https://docs.astral.sh/ruff",
        "source_repo": "https://github.com/astral-sh/ruff"
    },
    "python-lsp-ruff": {
        "type": "library",
        "version": "2.2.2",
        "manager": "pip",
        "source_repo": "https://github.com/python-lsp/python-lsp-ruff"
    },
    "ruffconfig": {
        "type": "symlink",
        "source": "DOT/languages/python/ruff.toml",
        "target": "~/.config/ruff/pyproject.toml"
    },
    "pylint": {
        "type": "exe",
        "version": "3.3.4",
        "installers": [
            "Pip"
        ],
        "homepage": "https://pylint.readthedocs.io/en/latest/",
        "source_repo": "https://github.com/pylint-dev/pylint"
    },
    "pylintrc": {
        "type": "symlink",
        "source": "DOT/languages/python/pylintrc",
        "target": "~/.config/pylint/pylintrc"
    },
    "mypy": {
        "type": "exe",
        "version": "1.15.0",
        "installers": [
            "Pip"
        ],
        "homepage": "https://www.mypy-lang.org/",
        "source_repo": "https://github.com/python/mypy"
    },
    "pylsp-mypy": {
        "type": "library",
        "version": "0.7.0",
        "manager": "pip",
        "source_repo": "https://github.com/python-lsp/pylsp-mypy"
    },
    "pylsp-rope": {
        "type": "library",
        "version": "0.1.17",
        "manager": "pip",
        "source_repo": "https://github.com/python-rope/pylsp-rope"
    },
    "debugpy": {
        "type": "library",
        "version": "1.8.12",
        "manager": "pip",
        "source_repo": "https://github.com/microsoft/debugpy"
    },
    "ts-python": {
        "type": "parser",
        "language": "python"
    }
}
