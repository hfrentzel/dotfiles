{
    "node": {
        "type": "exe",
        "version": "22.14.0",
        "installers": ["Tar"],
        "url":"https://nodejs.org/dist/v{version}/node-v{version}-linux-x64.tar.xz"
    },
    "typescript": {
        "type": "library",
        "version": "4.9.4",
        "manager": "npm",
        "homepage": "https://www.typescriptlang.org/",
        "source_repo": "https://github.com/Microsoft/TypeScript"
    },
    "eslint": {
        "type": "exe",
        "version": "8.32.0",
        "installers": ["Npm"],
        "homepage": "https://eslint.org/",
        "source_repo": "https://github.com/eslint/eslint"
    },
    "typescript-language-server": {
        "type": "exe",
        "version": "4.3.3",
        "installers": ["Npm"],
        "source_repo": "https://github.com/typescript-language-server/typescript-language-server"
    },
    "vscode-langservers-extracted": {
        "type": "exe",
        "command_name": "vscode-eslint-language-server",
        "version": "4.10.0",
        "installers": ["Npm"],
        "version_cmd": "NPM",
        "source_repo": "https://github.com/hrsh7th/vscode-langservers-extracted"
    },
    "ts-javascript": {
        "type": "parser",
        "language": "javascript"
    },
    "ts-tsx": {
        "type": "parser",
        "language": "tsx"
    },
    "ts-typescript": {
        "type": "parser",
        "language": "typescript"
    }
}
