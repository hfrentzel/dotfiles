{
    "vimrc": {
        "type": "symlink",
        "source": "DOT/vim/vimrc",
        "target": "CONFR/vim/vimrc"
    },
    "nvimconfig": {
        "type": "symlink",
        "source": "DOT/vim/nvim",
        "target": "CONFR/nvim"
    },
    "neovim": {
        "type": "exe",
        "command_name": "nvim",
        "version": "0.11.0",
        "installers": ["bh"],
        "homepage": "https://neovim.io",
        "source_repo": "https://github.com/neovim/neovim"
    },
    "tree-sitter-cli": {
        "type": "exe",
        "command_name": "tree-sitter",
        "version": "0.25.8",
        "installers": ["Npm"],
        "source_repo": "https://github.com/tree-sitter/tree-sitter"
    },
    "tree-sitter-config": {
        "type": "symlink",
        "source": "DOT/configs/tree-sitter-cli.json",
        "target": "CONF/tree-sitter/config.json"
    },
    "ts-vim": {
        "type": "parser",
        "language": "vim"
    },
    "ts-vimdoc": {
        "type": "parser",
        "language": "vimdoc"
    },
    "vim-language-server": {
        "type": "exe",
        "version": "2.3.1",
        "installers": ["Npm"],
        "version_cmd": "NPM",
        "source_repo": "https://github.com/iamcco/vim-language-server"
    },
    "pynvim": {
        "type": "library",
        "version": "0.4.3",
        "manager": "pip",
        "homepage": "https://pynvim.readthedocs.io/en/latest",
        "source_repo": "https://github.com/neovim/pynvim"
    },
    "command-t": {
        "type": "command",
        "cwd": "DOT/vim/nvim/pack/command-t/lua/wincent/commandt/lib",
        "run_script": "make",
        "check_script": "test -f commandt.so",
        "depends_on": ["submodules", "make", "gcc"]
    }
}
